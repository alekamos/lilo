<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="it.lifelog.mybatis.mappers.ModuleHeaderMapper" >
    
   <select id="selectByUser" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select 
    <include refid="Base_Column_List" />
    from MODULES_HEADER
    where ID_USER = #{idUser,jdbcType=INTEGER}
  </select>
  
   <select id="selectByUserAndType" resultMap="BaseResultMap" parameterType="it.lifelog.mybatis.bean.ModuleHeader" >
    select 
    <include refid="Base_Column_List" />
    from MODULES_HEADER
    where ID_USER = #{idUser,jdbcType=INTEGER}
    AND ID_TYPE = #{idType,jdbcType=INTEGER}
  </select>
  
  
  
</mapper>